#the question's command functions for Question ten, Button one
def question_ten_button_command1():
    global question_ten_button_variable_1
    global question_ten_count
    if question_ten_count == 0:
        question_ten_button_variable_1 = True
        question_ten_count += 1
    if question_ten_button_variable_1 == True:
        global gamescore
        gamescore += 1
        rng_for_question()
    elif question_ten_button_variable_1 == False:
        game_over()
    else:
        error.pack()
    time_limit_cancel()
    print( question_ten_count,question_ten_button_variable_1, gamescore)

